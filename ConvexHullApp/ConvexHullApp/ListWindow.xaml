<Window x:Class="ConvexHullApp.ListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConvexHullApp"
        mc:Ignorable="d"
        Title="ListWindow" Height="400" Width="600">
    <Grid>
        <Border BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Padding="10" Margin="10">
            <StackPanel x:Name="ListWindowStackPanel" HorizontalAlignment="Center">
            </StackPanel>
        </Border>
    </Grid>
</Window>
